{
  "name": "@themed.js/vue",
  "version": "0.1.0",
  "description": "Vue bindings for Themed.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "theme",
    "theming",
    "vue",
    "composables",
    "css-variables",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.4.0"
  },
  "dependencies": {
    "@themed.js/core": "workspace:*"
  },
  "devDependencies": {
    "vue": "^3.5.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  }
}
