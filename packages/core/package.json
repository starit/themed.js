{
  "name": "@themed.js/core",
  "version": "0.1.0",
  "description": "Framework-agnostic theme management library with AI-powered theme generation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "theme",
    "theming",
    "css-variables",
    "dark-mode",
    "ai",
    "design-tokens"
  ],
  "author": "Starit <starit.public@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/starit/themed.js"
  },
  "bugs": {
    "url": "https://github.com/starit/themed.js/issues"
  },
  "homepage": "https://github.com/starit/themed.js#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  }
}
